{
  "productName": "rust-mail2",
  "version": "0.1.0",
  "identifier": "com.spartajet.rust-mail",
  "build": {
    "frontendDist": "../dist/spa",
    "devUrl": "http://localhost:9000",
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build"
  },
  "app": {
    "windows": [
      {
        "title": "Rust Mail",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "resources/*"
    ]
  },
  "$schema": "../node_modules/@tauri-apps/cli/schema.json"
}